# .env file for BlueJay Backend

# OpenAI API Key (from OpenAI dashboard)
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Assistant ID (from your BlueJay Assistant setup)
OPENAI_ASSISTANT_ID=asst-xxxxxxxxxxxxxxxxxxxxxxxx

# Redis database connection URL
REDIS_URL=redis://default:yourpassword@yourredisinstanceid:6379

# Flask session secret (make it random and secret)
FLASK_SECRET_KEY=supersecretkey123456789